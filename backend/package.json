{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/build/server",
  "scripts": {
    "prismaSeed": "ts-node ./prisma/seeds/states.ts && ts-node ./prisma/seeds/categories.ts && ts-node ./prisma/seeds/address.ts && ts-node ./prisma/seeds/users.ts && ts-node ./prisma/seeds/products.ts && ts-node ./prisma/seeds/orders.ts",
    "prestart": "npx prisma generate && npx prisma migrate dev",
    "start": "npm run build && node ./dist/server.js && npm run prestart",
    "debug": "nodemon --watch \"./src/**\" --ext \"ts,js\" --ignore \"src/**/*.spec.ts,src/**/*.json\" --exec \"ts-node src/index.ts\"",
    "dev": "tsnd ./src/server.ts",
    "build": "tsc"
  },
  "prisma": {
    "seed": "npm run prismaSeed"
  },
